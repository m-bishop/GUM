package gum;

import java.util.Comparator;

public class InitiativeComparator implements Comparator<Player> {

	@Override
	public int compare(Player p1, Player p2) {
        if(p1.getInitiative() < p2.getInitiative())return 1;
        if(p1.getInitiative() < p2.getInitiative())return -1;
		return 0;
	}

}
